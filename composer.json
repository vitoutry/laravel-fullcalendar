{
    "name": "vitoutry/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Try Vitou",
            "email": "vitoutry@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-0": {
            "Vitoutry\\LaravelFullcalendar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vitoutry\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Vitoutry\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
